
public class square {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int y=0;y<=12;y++)
	    {
	        if(y==0||y==12)
	        {
	            System.out.print( "*************\n" );
	            if(y==12)
	            {
	              
	                break;
	            }
	        }

	     
	        for(int x=0;x<=12;x++)
	        {
	            if(x==0||x==12)
	            {
	                System.out.print( "*" );
	                if(x==12)
	                {
	                	System.out.println();
	                }
	            }
	            else
	            {
	                System.out.print( " " );
	            }
	        }

	    }
											}

	

}
