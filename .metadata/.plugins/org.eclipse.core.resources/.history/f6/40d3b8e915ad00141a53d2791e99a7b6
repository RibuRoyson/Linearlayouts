
public class triangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
for(int y=0;y<5;y++)
{
	if(y==0)
	{
		System.out.println("***********/n");
		
	}else if(y==1)
	{
		System.out.println("*/t/t/t*");
	}else if(y==2)
	{
		System.out.println("*/t/t*");

	}
	else if(y==3)
	{
		System.out.println("*/t*");

	}
}
	}

}
